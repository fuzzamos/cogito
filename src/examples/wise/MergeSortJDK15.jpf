@using cogito
shell=edu.cmu.sv.isstac.cogito.Cogito

cogito.training.target.args=7,7
cogito.prediction.target.args=50

target=wise.MergeSortJDK15
symbolic.method=wise.java15.util.Arrays.sort(con)

symbolic.min_int=-100
symbolic.max_int=100

symbolic.dp=z3
#symbolic.dp=z3inc
#listener=gov.nasa.jpf.symbc.numeric.solvers.IncrementalListener

symbolic.optimizechoices=false



